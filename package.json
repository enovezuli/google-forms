    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "winston": "^3.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "client": "npm start",
    "server": "cd server && node index.js",
    "dev": "npm run check-node-version && concurrently \"npm run server\" \"npm run client\"",
    "check-node-version": "node -v | grep -E '^v14\\.' && echo \"Node version is suitable for development.\" || (echo \"Your project requires Node.js version 14.x.x. Please upgrade or install Node.js 14 and try again.\" && exit 1)"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^6.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}